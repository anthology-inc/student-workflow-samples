        <FlowStep.Next>
          <FlowDecision x:Name="__ReferenceID3" Condition="[ISIR.AdditionalFields(&quot;CPSpushedISIRflag&quot;)=(&quot;Y&quot;)]" sap2010:WorkflowViewState.IdRef="FlowDecision_2">
